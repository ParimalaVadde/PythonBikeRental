CREATE TABLE IF NOT EXISTS sds_staging.business_entity_details
(
    stg_business_entity_id   varchar not null primary key,
    business_entity_name varchar,
    company_structure         varchar,
    market_segment_type  varchar,
    industry_type        varchar,
	year_incorporated varchar,
	reported_annual_revenue  numeric,
    is_active            boolean,
    last_updated_by      varchar,
    last_updated_date    timestamp,
    created_by           varchar,
    created_date         timestamp,
	business_validation_reason varchar
);


