- type: RDSProxy
  name: proxy
  action: apply
  connectsTo:
    - type: RDSAuroraPostgres
      name: xbsdtestdb
  properties:
    interactive_reader_endpoint: true
    recovery_window_in_days: 0
    global_config:
      enabled: false
      secondary_region: ""
      secondary_vpc_subnet_names: []
      primary_secrets_vpc_endpoint_dns_names: []
      secondary_secrets_vpc_endpoint_dns_names: []
