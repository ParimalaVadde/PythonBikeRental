do i hv to go with indivisual function for each column like below or how to fix it

def explode_associated_people(row):
    try:
        # Convert the string representation of the list to an actual list
        people_list = ast.literal_eval(row['associated_people'])
        # Create a DataFrame from the list of dictionaries
        exploded_df = pd.DataFrame(people_list)
        exploded_df['vendor_name_cleaned'] = row['vendor_name_cleaned']  # Add vendor name to each row
 
        # Explode the 'titles' column into multiple rows
        exploded_df = exploded_df.explode('titles')
        return exploded_df
    except Exception as e:
        print(f"Error processing row: {e}")
        return pd.DataFrame()
		
		
def explode_card_revenue(data):
    """
    Explodes a nested dictionary into a flat dictionary with multiple columns.
 
    Args:
        data (str): A string representation of a dictionary.
 
    Returns:
        dict: A flattened dictionary with keys as column names and values as the corresponding data.
    """
    try:
        # Convert the string representation of the dictionary to an actual dictionary
        card_revenue_data = ast.literal_eval(data)
 
        # Extract the relevant fields from the dictionary
        flattened_data = {
            'end_date': card_revenue_data.get('end_date'),
            'date_accessible': card_revenue_data.get('date_accessible'),
            '1m_start_date': card_revenue_data.get('1m', {}).get('start_date'),
            '1m_average_monthly_amount': card_revenue_data.get('1m', {}).get('average_monthly_amount'),
            '3m_start_date': card_revenue_data.get('3m', {}).get('start_date'),
            '3m_average_monthly_amount': card_revenue_data.get('3m', {}).get('average_monthly_amount'),
            '12m_start_date': card_revenue_data.get('12m', {}).get('start_date'),
            '12m_average_monthly_amount': card_revenue_data.get('12m', {}).get('average_monthly_amount'),
        }
        return flattened_data
    except Exception as e:
        print(f"Error processing data: {e}")
        return {
            'end_date': None,
            'date_accessible': None,
            '1m_start_date': None,
            '1m_average_monthly_amount': None,
            '3m_start_date': None,
            '3m_average_monthly_amount': None,
            '12m_start_date': None,
            '12m_average_monthly_amount': None,
        }
